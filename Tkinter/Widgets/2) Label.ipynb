   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "window = tk.Tk() # initialize the window\n",
    "window.title('Label') # title\n",
    "window.geometry('600x1000') # resolution\n",
    "\n",
    "label = ttk.Label(master = window, text = 'This is a label')\n",
    "label.pack()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "window.mainloop()"
   ]
  }
